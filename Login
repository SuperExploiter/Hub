local function JUDYRF_fake_script() 
	local script = Instance.new('LocalScript', LoginButton)

	script.Parent.MouseButton1Down:Connect(function()
		if script.Parent.Parent.Username.Text == "petsimx" and script.Parent.Parent.Password.Text == "eatpoopbooterahad" then
			local sound = Instance.new("Sound", game.Workspace)
			sound.SoundId = "rbxassetid://2027986581"
			sound.Looped = false
			sound:Play()
			wait(0.1)
		game.CoreGui.LoginGui:Remove()
			loadstring(game:HttpGet("https://raw.githubusercontent.com/SuperExploiter/Petsimx/main/Script.lua"))()
		end
		if script.Parent.Parent.Username.Text == "bwergwergwrgr" and script.Parent.Parent.Password.Text == "bwergwergwergwergwerg" then
			local sound = Instance.new("Sound", game.Workspace)
			sound.SoundId = "rbxassetid://2027986581"
			sound.Looped = false
			sound:Play()
			wait(0.1)
		game.CoreGui.LoginGui:Remove()
			loadstring(game:HttpGet(LoginLoadstring,true))()
		end
		if script.Parent.Parent.Username.Text == "cwergwergwergwergwer" and script.Parent.Parent.Password.Text == "ergwergwergwergwergewc" then
			local sound = Instance.new("Sound", game.Workspace)
			sound.SoundId = "rbxassetid://2027986581"
			sound.Looped = false
			sound:Play()
			wait(0.1)
	game.CoreGui.LoginGui:Remove()
			loadstring(game:HttpGet(LoginLoadstring,true))()
		end
		if script.Parent.Parent.Username.Text == "dgewrgwergwergwregw" and script.Parent.Parent.Password.Text == "dfergergwergergwer" then
			local sound = Instance.new("Sound", game.Workspace)
			sound.SoundId = "rbxassetid://2027986581"
			sound.Looped = false
			sound:Play()
			wait(0.1)
	game.CoreGui.LoginGui:Remove()
			loadstring(game:HttpGet(LoginLoadstring,true))()
		end
		if script.Parent.Parent.Username.Text == "Xester" and script.Parent.Parent.Password.Text == "pro" then
			local sound = Instance.new("Sound", game.Workspace)
			sound.SoundId = "rbxassetid://2027986581"
			sound.Looped = false
			sound:Play()
			wait(0.1)
	game.CoreGui.LoginGui:Remove()
		loadstring(game:HttpGet(LoginLoadstring,true))()
		else
			local sound1 = Instance.new("Sound", game.Workspace)
			sound1.SoundId = "rbxassetid://130840811"
			sound1.Looped = false
			sound1:Play()
			script.Parent.Parent.Username.Text = "Username"
			script.Parent.Parent.Password.Text = "Password"
		end
	end)
end
coroutine.wrap(JUDYRF_fake_script)()
local function HSHTNV_fake_script()
	local script = Instance.new('LocalScript', Frame)

	local UserInputService = game:GetService("UserInputService")

	local gui = script.Parent

	local dragging
	local dragInput
	local dragStart
	local startPos

	local function update(input)
		local delta = input.Position - dragStart
		gui.Position = gui:TweenPosition(UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y), 'Out', 'Linear', 0.09, true); 
	end

	gui.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position

			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)

	gui.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)

	UserInputService.InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			update(input)
		end
	end)
end
coroutine.wrap(HSHTNV_fake_script)()
